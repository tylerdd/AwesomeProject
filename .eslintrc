{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "babel/object-shorthand": 0,
    "comma-dangle": 0,
    "constructor-super": 2,
    "func-names": 0,
    "guard-for-in": 0,
    "id-length": 0,
    "key-spacing": 0,
    "keyword-spacing": [
      2,
      {
        "after": true
      }
    ],
    "linebreak-style": 0,
    "new-cap": 1,
    "no-console": 0,
    "no-magic-numbers": 0,
    "no-script-url": 0,
    "no-undef": 2,
    "no-unused-vars": [
      2,
      {
        "args": "none",
        "vars": "all"
      }
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "one-var": [
      2,
      {
        "initialized": "never"
      }
    ],
    "prefer-const": 0,
    "react-native/no-color-literals": 0,
    "react-native/no-inline-styles": 0,
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "react/forbid-prop-types": [
      2,
      {
        "forbid": [
          "any"
        ]
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/sort-comp": 0,
    "semi": [
      2,
      "never"
    ],
    "space-before-blocks": [
      2,
      {
        "functions": "always",
        "keywords": "always"
      }
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "spaced-comment": 0,
    "vars-on-top": 0
  }
}
